pipeline {
    agent {
        node {
            label 'maven'
        }
    }
environment {
    PATH = "/opt/apache-maven-3.9.5/bin:$PATH"
}
    stages {
        stage('build') {
            steps {
            #     git branch: 'main', url: 'https://github.com/techsysvbo/tweet-trend.git'
                  sh 'mvn clean deploy'
            }
        }
    }
}
